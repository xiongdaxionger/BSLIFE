page{
  font-size: 14px;
}
.recash-item{
  display: flex;
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
}
.recash-item-l{
  width: 80%;
}
.recash-item-r{
  width: 20%;
  text-align: right;
}
.right-img{
  width: 40rpx;
  height: 40rpx;
}
.recash-price{
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
}
.gray-words{
  color: #bbb;
}
.recash-buttom{
  margin-top: 20px;
  padding: 20rpx;
}
.recash-btn{
  background: #dc4c00;
  color: #fff;
}
.return-money{
  text-align: center;
  position: fixed;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: #f6f6f6;
}
.return-time{
  margin-top: 10px;
}
.disable{
  background-color: #DCDCDC;
  color: gray;
}