page{
  font-size: 14px;
  background: #f5f5f5;
}
.group-btns{
  display: flex;
  padding: 20rpx;
}
.invite-item{
  width: 100%;
  color: #a8a8a8;
  text-align: center;
}
.invite-img{
  width: 140rpx;
  height: 140rpx;
  margin-bottom: 10rpx;
}
button{
  border: none;
  margin:0px 0px 20rpx 0rpx;
  height: 140rpx;
  width: 100%;
  padding:0;
  background: #f5f5f5;
}
button::after{
  border: none;
}
.button-hover{
  background: #f5f5f5;
}