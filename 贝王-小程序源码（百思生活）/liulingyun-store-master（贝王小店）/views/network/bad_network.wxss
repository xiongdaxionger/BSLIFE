
/*背景*/
.background{
    position: absolute;
    width:100%;
    height: 100vh;
    background-color:#EDEDED;
    display: flex;
    flex-flow: column wrap;
    justify-content: center;
    align-items: center;
}

/*图标*/
.image{
    width:100px;
    height: 100px;
    margin-bottom: 20px;
}
/*文字*/
.text{
    font-size:16px;
    color:gray;
    text-align:center;
    margin-bottom: 5px;
    font-weight: normal;

}
